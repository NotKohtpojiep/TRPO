<Window x:Class="FileEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mv="clr-namespace:FileEditor.ViewModels"
        xmlns:pages="clr-namespace:FileEditor.Pages"
        xmlns:local="clr-namespace:FileEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl x:Name="TabControl" Margin="0 0 0 20">
            <TabItem Header="Задание 1">
                <pages:FirstQuest />
            </TabItem>
            <TabItem Header="Задание 2">
                <pages:SecondQuest />
            </TabItem>
            <TabItem Header="Задание 3">
                <pages:ThirdQuest />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
